{
  "name": "park-web",
  "version": "0.0.1",
  "description": "Parking web",
  "main": "build/modules/main.module.js",
  "scripts": {
    "cleanup": "rimraf src/app/**/*.ngfactory.ts src/app/**/*.ngstyle.ts src/app/**/*.ngsummary.json build dist/*",
    "cleanjs": "rimraf src/app/**/*.js",
    "explorer": "source-map-explorer ./dist/main.bundle.js ./dist/main.bundle.js.map",
    "ngc": "ngc",
    "serve": "http-server ./dist -a 0.0.0.0 -p 9000",
    "start": "npm run cleanup && npm run ngc && concurrently -r \"webpack-dev-server --config webpack.aot.config.js\" \"node ./bin/ngc-watch.js\"",
    "start-prod": "npm run ngc && npm run build && npm run serve",
    "build": "npm run cleanup && npm run ngc && webpack --config webpack.aot.config.js -p",
    "build-jit": "npm run cleanup && npm run ngc && webpack --config webpack.jit.config.js -p",
    "deploy": "gh-pages -d dist",
    "aot": "npm run cleanup && npm run ngc && concurrently -r \"webpack-dev-server --config webpack.aot.config.js\" \"node ./bin/ngc-watch.js\"",
    "jit": "npm run cleanup && webpack-dev-server --config webpack.jit.config.js"
  },
  "keywords": [
    "angular2",
    "aot",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+http://git.parksolutions.ru/core/park-web.git"
  },
  "author": {
    "name": "ZakirovRR",
    "email": "zakirovrr@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/forms": "^2.4.10",
    "@angular/http": "^2.4.10",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-browser-dynamic": "^2.4.10",
    "@angular/router": "^3.4.10",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "ng2-bootstrap": "1.4.2",
    "openlayers": "4.0.1",
    "rxjs": "5.2.0",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.10",
    "@angular/platform-server": "2.4.10",
    "@types/node": "7.0.8",
    "@types/openlayers": "4.0.1",
    "@types/geojson": "1.0.0",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "chokidar": "1.6.1",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.1",
    "file-loader": "0.10.1",
    "gh-pages": "0.12.0",
    "json-loader": "^0.5.4",
    "http-server": "0.9.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "typescript": "2.1.5",
    "url-loader": "0.5.8",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  }
}
